<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lin.drawable">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" />
        <activity android:name=".LayerDrawable" />
        <activity android:name=".Activity_Level" />
        <activity android:name=".TransitionDrawable"></activity>
        <activity android:name=".InsertDrawableActivity"></activity>
        <activity android:name=".ClipDrawableActivity">
